<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="../../favicon.ico">

    <title>Test harness for Timeli</title>

    <!-- Bootstrap core CSS -->
    <link href="/css/bootstrap.min.css" rel="stylesheet">
    <link href="/css/bootstrap-theme.min.css" rel="stylesheet">
    <link href="/css/colorbox.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="/css/promptli.css" rel="stylesheet">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>

  <body>

    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="#">Timeli Test</a>
        </div>
        <div id="navbar" class="collapse navbar-collapse">
          <ul class="nav navbar-nav">
            <li class="active"><a href="#">Home</a></li>
            <li><a href="#about">About</a></li>
            <li><a href="#contact">Contact</a></li>
          </ul>
        </div><!--/.nav-collapse -->
    </nav>

    <div class="container">

      <div class="starter-template">
        <h1>Timeli SDK API Test Harness</h1>
        <!--<p class="lead">coming soon to a screen near you</p>-->
        <!--<button id="sdk-authenticate">Authenticate</button>
        <button id="sdk-asset">List Assets</button>
        <button id="test-log">Test</button>-->
         <div class="control">

             <label>Select Version</label>
             <select class="version">
                 <option value="0">None</option>
                 <option value="v1">Version 1</option>
                 <option value="v2">Version 2</option>
                 <option value="a">Regression Mode</option>
             </select>
              <label>Select Resource</label>
              <select class="resources">
                <option value="0">Select Resource</option>
              </select>
              <label>Select Method</label>
              <select class="methods">
                <option value="0">Select Method</option>
              </select>
               <button class="code-button">Test Code</button>
               <div class="code-box show-box">
                   <p style="margin:10px;">Please enter your test code here.<br> click 'Run' when done.</p>
                   <textarea name="code" cols="40" rows="5" ... ></textarea>
                   <input name="script_name_for_code" type="hidden">
                   <div>
                        <button class="run-button">Run</button>
                       <button class="save-button">Save</button>
                        <button class="clear-button">Clear</button>
                       <button class="close-button">Close</button>
                       <button class="get-button">Get</button>
                   </div>
               </div>
           </div>
          <!--<label for="command">Execute:</label>
          <input type="text" name="command" value="">-->
         </div>
          <div class="show-box go-box">
              <a class="pin-control unpinned"><div class="pin">P</div></a>
            <p style="margin:10px;text-align:center;">Please enter values for method arguments. <br> Enter ALL values or NONE. Click 'Go' when done.</p>
            <div class="centered params" style="margin-top:20px;"></div>
              <div align="center">
                <button class="go-button show-button">Go</button>
                <button class="close-button show-button">Close</button>
              </div>
          </div>
          <div id="busy" style="display:none"><img src="/images/loading.gif"></div>
          <button class="clear-transcript">Clear Transcript</button>
          <!--<input style="margin-right:5px; margin-left:100px;vertical-align:top;" type="checkbox" name="mode" value="mode">Use Regression Mode-->
          <button class="recording" style="margin-left:50px;">Start Recording</button>
          <button class="recording roundbutton"></button>
          <button class="reset-recording" style="margin-left:35px;">Reset Recording</button>
          <button class="save-recording" style="margin-left:35px;">Save Recording</button>
          <button class="run-recording" style="margin-left:35px;">Run Script</button>
          <button class="manage-scripts" style="margin-left:35px;">Manage Scripts</button>
          <div id="log" class="console"><p align="left"></p></div>
      </div>

    </div><!-- /.container -->
    <div style="display:none;">
        <div class="save">
            <input type="text" name="name" placeholder="Name of Test">
            <p class="error-msg"></p>
            <textarea name="description" rows="2" cols="50" placeholder="Description"></textarea>
            <!--<input type="text" name="description" placeholder="Description">-->
            <button class="popupsave">Save</button>
            <button class="xbutton">Close</button>
        </div>
        <div class="get">
            <input type="text" name="name" placeholder="Name of Script">
            <button class="popupget">Get</button>
            <button class="xbutton">Close</button>
        </div>
        <div class="version-reset-warn">
            <p></p>
            <button class="popupOK">OK</button>
            <button class="xbutton">Cancel</button>
        </div>
        <div class="list-of-scripts">
            <p><a class="all-scripts">List All</a></p>
            <table border="1">
                <tr>
                    <th>Name</th><th>Description</th>
                </tr>
            </table>
        </div>
        <div class="recorded-results">
            <ul>
            </ul>
        </div>
    </div>


    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="/js/jquery.colorbox-min.js"></script>
    <!--<script src="http://demo.timeli.io/static/js/public/sdk.js"></script>-->
    <!--<script src="/js/sdk.js"></script>-->
    <script src="/js/promptli.js"></script>
    <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
    <script src="/js/ie10-viewport-bug-workaround.js"></script>
    <script src="/js/sdkv2.js"></script>
    <script>
        APP.SDK.cloneInto(sdk['v2'].sdk);
        sdk['v2'].sdk.APP.SDK._version = 'v2';
    </script>
    <script src="/js/sdkv1.js"></script>
    <script>
        APP.SDK.cloneInto(sdk['v1'].sdk);
        sdk['v1'].sdk.APP.SDK._version = 'v1';
    </script>
    <% if (typeof(run_auto) != "undefined") { %>
    <script>
        var run_auto = "<%=run_auto%>";
    </script>
    <% } %>
    <script>
        var testconfig = JSON.parse('<%-JSON.stringify(testconfig)%>');
    </script>
  </body>
</html>
